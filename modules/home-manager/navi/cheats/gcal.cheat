% gcal

# month calendar
;gum spin --spinner dot --title "Fetching..." --
gcalcli calm --monday --military --width 15 | less -r

# week calendar
; gum spin --spinner dot --title "Fetching..." --
gcalcli calw --monday --military | less -r

# agenda
; gum spin --spinner dot --title "Fetching..." --
gcalcli agenda | less -r
